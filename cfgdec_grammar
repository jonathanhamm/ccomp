<start>->
    <statements>
  
<statements>->
    <statement>
    <statements>
    |
    ε

<statement>->
    nonterm <idsuffix> assignop <expression>
    |
    if <expression> then <statements> <else>
    |
    id \(<paramlist>\)
    
<else>->
    else <statements> fi
    |
    fi
    |
    <elif>

<elif>->
    elif <expression> then <statements>

<expression> ->
	<simple_expression> <expression'>
<expression'> ->
	relop <simple_expression>
	|
	ε	

<simple_expression> ->
	<sign> <simple_expression>
	|
	<term> <simple_expression'>
    
<simple_expression'> ->
	addop <term> <simple_expression'>
	|
	ε

<term> ->
	<factor> <term'>

<term'> ->
	mulop <factor> <term'>
	|
	ε

<factor> ->
	id <idsuffix>
    |
    nonterm  <idsuffix>
	|
	num
	|
	not <factor>
    |
    \( <expression> \)
    |
    code <code>

<factor'> ->
	num
	|
	ε

<idsuffix> ->
    <factor'> <dot>
    |
    \( <paramlist> \)
    |
    \[ <expression> \] <dot>

<dot> ->
    \. id <range>
    |
    ε
    
<range> ->
    \.\. id \[ num \] \. id
    |
    ε
        

<code>
    code <code>
    |
    ε

<paramlist> ->
    <expression> <paramlist'>
    |
    ε

<paramlist'> ->
    , <expression> <paramlist'>
    |
    ε

<sign> ->
	\+ | \-
