<start>=>
	<nonterminal>
	<nonterminals>

<nonterminals>=>
	<nonterminal> <nonterminals>
	|
	\E

<nonterminal>=>
	nonterm prodsym <production> <productions> <decoration>

<productions>=>
	UNION <production> <productions>
	|
	\E

<production>=>
	token <tokens>

<tokens>=>
	token <tokens>
	|
	\E

<decoration>=>
	{ * }
	|
	\E