<start>=>
	<nonterminal>
	<nonterminals>

<nonterminals>=>
	<nonterminal>
	|
	\E

<nonterminal>=>
	nonterm prodsym <production> <productions> <decoration>

<productions>=>
	UNION <production>
	|
	\E

<production>=>
	token <tokens>

<tokens>=>
	token <token>
	|
	\E

<decoration>=>
	[ * ]
	|
	\E