<type>=>
    <char> file 
    {
        <type>.grammartype := <char>.grammartype;
        <type>.val := file 
    }
    | 
    - <word> = val
    {
        <type>.grammartype := <word>.grammartype;
        <type>.val := val;
    }
    
<char>=>
    r 
    {
        <char>.grammartype := regex;
    }
    |
    p
    {
        <char>.grammartype := cfg;
    }
    |
    f

<word>=>
    regex
    {
        <word>.grammartype := regex;
    }
    |
    cfg
    {
        <word>.grammartype := cfg;
    }
    |
    source
    |
