<start>=>
    <keywords>
    <tokens>

<keywords>=>
    terminal <keywords>
    |
    €

<tokens>=>
    <texp> <tokens'>
<tokens'>=>
    EOL <texp> <tokens'>
    |
    €

<texp>=>
    nonterm prodsym <expression>
<expression>=>
    <term> <closure> <expression'>
<expression'>=>
    <union> <expression>
    |
    €

<term>=>
    ( <expression> )
    |
    terminal <union> <expression'>
    |
    nonterm <union> <expression'>

<union>=>
    union
    |
    €


<closure>=>
    * | + | ? | | <closure> | €
    