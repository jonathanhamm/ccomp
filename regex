<start>=>
    <keywords>
    <tokens>

<keywords>=>
    terminal <keywords>
    |
    €

<tokens>=>
    <texp> <tokens'>
<tokens'>=>
    EOL <texp> <tokens'>
    |
    €

<texp>=>
    nonterm prodsym <expression>
<expression>=>
    <term> <closure> <expression'>
<expression'>=>
    union <expression>
    |
    <expression>
    |
    €

<term>=>
    ( <expression> )
    |
    terminal <expression'>
    |
    nonterm <expression'>


<closure>=>
    * | + | ? | | <closure> | €
    