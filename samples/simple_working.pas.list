     1: program bob (input, output);
     2: var a : integer;
     3: var b : array[1..10] of real;
     4: var c : array[1..10] of integer;
     5: var d : array[1..11] of integer;
     6: var e : real;
     7: var f : array[1..10] of real;
     8: begin
     9:     c := c;
    10:     b := f;
    11:     while a + b[a] <= 10 do
    12:     begin
    13:         a := a + c[a+1];
    14:         if a = e then
    15:             begin
    16:                 e := c[32];
    17:                 c[31] := d[e <> 2]
    18:             end
    19:         else
    20:             e := c[31];
    21:     d[10] := c[d[c[3 mod 4]]]
    22:     end
    23: end.
